<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="PrimaryFlatButton" parent="Theme.AppCompat.Light">
        <item name="android:buttonStyle">
            @style/Widget.AppCompat.Button.Borderless.Colored
        </item>
        <item name="colorAccent">#FFAB40</item>
    </style>

    <style name="Custom1" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/custom1Primary</item>
        <item name="colorPrimaryDark">@color/custom1PrimaryDark</item>
        <item name="colorAccent">@color/custom1Accent</item>
    </style>

    <style name="Custom2" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/custom2Primary</item>
        <item name="colorPrimaryDark">@color/custom2PrimaryDark</item>
        <item name="colorAccent">@color/custom2Secondary</item>

    </style>


</resources>
